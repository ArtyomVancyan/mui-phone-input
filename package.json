{
  "version": "0.1.0-alpha",
  "name": "mui-phone-input",
  "description": "Advanced, highly customizable phone input component for Material UI.",
  "keywords": [
    "mui",
    "react",
    "phone",
    "input",
    "number",
    "material",
    "advanced",
    "component",
    "material-ui",
    "customizable",
    "phone-number"
  ],
  "homepage": "https://github.com/typesnippet/mui-phone-input",
  "bugs": {
    "url": "https://github.com/typesnippet/mui-phone-input/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typesnippet/mui-phone-input"
  },
  "files": [
    "core",
    "index*",
    "types*",
    "styles*",
    "LICENSE",
    "resources",
    "README.md"
  ],
  "scripts": {
    "rename": "bash -c 'for file in $0/*.js; do mv $file \"${file%.js}.$1.js\"; done'",
    "build:core": "tsx scripts/environment-build.ts core @material-ui/core",
    "build:material": "tsx scripts/environment-build.ts . @mui/material",
    "prebuild": "rm -r core resources index* types* styles* || true",
    "build": "npm run build:core && npm run build:material",
    "test": "tsx scripts/environment-test.ts $1",
    "postpack": "tsx scripts/prepare-package.ts",
    "postbuild": "cp -r src/resources ."
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.6 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "react-phone-hooks": "0.1.0-beta.1"
  },
  "devDependencies": {
    "@emotion/styled": "^11.11.0",
    "@material-ui/core": "^4.0.0",
    "@mui/joy": "^5.0.0-beta.18",
    "@mui/material": "^5.0.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/react": "^17.0.71",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react-hook-form": "^7.49.2",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2"
  }
}
